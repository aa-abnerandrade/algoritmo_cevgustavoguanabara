algoritmo "Contador"
var
contador, max: inteiro
inicio
Escreval ("VAMOS CONTAR?")

Escreva ("Até que número deseja contar? -> ")
Leia (max)
Escreval ("Ok, vamos contar até ", max, ".")

contador <- 0
Enquanto (contador <=,0 max) faca
         Escreval (contador)
         contador <- contador + 1
FimEnquanto

Escreval ("Terminei de contar!")
fimalgoritmo